{
  "name": "rendertune",
  "productName": "RenderTune",
  "version": "2.0.0",
  "description": "A simple starter project to get up and developing quickly with the blend of React and Electron JS for building a Native Desktop App on various OS Platforms like Windows, Linux and macOS using Electron Builder.",
  "main": "main.js",
  "author": "Souleh Shaikh <author.souleh@thegeekytutor.com>",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/MartinBarker/rendertune.git"
  },
  "browser": {
    "fs": true,
    "path": false,
    "os": false
  },
  "scripts": {
    "dev": "concurrently \"npm run start\" \"npm run electron\"",
    "start": "npx cross-env BROWSER=none react-scripts start",
    "electron": "wait-on tcp:127.0.0.1:3000 -v && electron ."
  },
  "dependencies": {
    "@emotion/react": "^11.10.8",
    "@emotion/styled": "^11.10.8",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.12.3",
    "babel-loader": "8.0.4",
    "buffer": "^6.0.3",
    "cross-env": "^7.0.3",
    "electron-is-dev": "^2.0.0",
    "electron-is-packaged": "^1.0.2",
    "electron-notarize": "^1.2.2",
    "electron-scripts": "^1.1.0",
    "electron-serve": "^1.0.0",
    "electron-updater": "^5.3.0",
    "express": "^4.18.2",
    "ffmpeg-ffprobe-static": "^4.4.0-rc.11",
    "fs": "0.0.1-security",
    "googleapis": "^126.0.1",
    "http": "^0.0.1-security",
    "material-react-table": "^1.11.1",
    "moment": "^2.29.4",
    "music-metadata-browser": "^2.5.10",
    "open": "^9.1.0",
    "path": "^0.12.7",
    "react-router-dom": "^6.20.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "concurrently": "^5.3.0",
    "electron": "^22.0.0",
    "electron-builder": "^23.6.0",
    "eslint": "^8.39.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^5.0.1",
    "wait-on": "^5.2.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
